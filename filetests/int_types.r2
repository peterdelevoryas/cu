fn printf(fmt: *i8, ...);

fn main() -> i32 {

    // check: 127
    let x: i8 = 127;
    printf("%hhd\n", x);

    // nextln: 32767
    let x: i16 = 32767;
    printf("%hd\n", x);

    // nextln: 2147483647
    let x: i32 = 2147483647;
    printf("%d\n", x);

    // nextln: 9223372036854775807
    let x: i64 = 9223372036854775807;
    printf("%ld\n", x);

    return 0;
}
