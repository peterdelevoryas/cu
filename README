type B struct {
    x: i32,
}

type A struct {
    b: B,
}

fn make_a(b: B) -> A {
    return {
        b: b,
    };
}

fn make_b(x: i32) -> B {
    return {
        x: x,
    };
}

fn printf(fmt: *i8, ...);

fn main(argc: i32, argv: **i8) -> i32 {
    let a = make_a(make_b(1));
    printf("a.b.x = %d\n", a.b.x);
    return 0;
}
