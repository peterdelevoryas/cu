type info struct {
    gender: *i8,
    age: i32,
}

type person struct {
    name: *i8,
    info: info,
}

fn new_info(gender: *i8, age: i32) -> info {
    return {
        gender: gender,
        age: age,
    };
}

fn printf(fmt: *i8, ...);

fn main(argc: i32, argv: **i8) -> i32 {
    let bob: person = {
        name: "bob",
        info: new_info("male", 32),
    };
    printf("bob.name: %s\n", bob.name);
    printf("bob.info.gender: %s\n", bob.info.gender);
    printf("bob.info.age: %d\n", bob.info.age);
    return 0;
}
